namespace org.example.basic

asset Account identified by accountId {
  o String accountId
  --> Customer owner
  o Double balance
}

participant Customer identified by customerId {
  o String customerId
  o String firstName
  o String lastName
}

transaction AccountTransfer {
  --> Account to
  --> Account from
  o Double amount
}

event TransactionComplete {
  --> Account asset
  o Double oldValue
  o Double newValue
}